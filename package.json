{
  "version": "1.8.3",
  "name": "babel-plugin-react-docgen",
  "description": "Babel plugin to add react-docgen info into your code",
  "repository": "https://github.com/storybooks/babel-plugin-react-docgen",
  "author": "Madushan Nishantha <j.l.madushan@gmail.com>",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "mocha": "^2.0.0"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src -d lib",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run test && npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "docs",
    "docgen",
    "babel-plugin"
  ],
  "dependencies": {
    "babel-types": "^6.24.1",
    "lodash": "^4.17.0",
    "react-docgen": "https://github.com/NikhilVerma/react-docgen/archive/v3.0.0-beta12.tar.gz"
  },
  "license": "MIT"
}
